programa {
  funcao inicio() {
    escreva ("Digite seu nome:")
    cadeia Nome
    leia(Nome)
    escreva ("Digite sua idade:")
    inteiro Idade
    leia(Idade)
    escreva ("Digite sua altura:")
    real Altura
    leia(Altura)
    escreva ("Nome: ", Nome, "\n", "Idade: ", Idade, "\n", "ALtura: ", Altura)
  }
}
